{
  "name": "semblance",
  "version": "1.0.0",
  "description": "The AI that runs the Reality Engine",
  "main": "dist/src/index.js",
  "author": "SirH",
  "type": "module",
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "@sapphire/framework": "^3.0.0-next.87e5cdc.0",
    "@top-gg/sdk": "^3.1.3",
    "bufferutil": "^4.0.6",
    "discord-api-types": "^0.36.2",
    "discord.js": "^13.8.1",
    "dotenv": "^16.0.1",
    "erlpack": "github:discord/erlpack",
    "fastify": "^4.2.1",
    "node-schedule": "^2.1.0",
    "source-map-support": "^0.5.21",
    "twitter": "^1.7.1",
    "twitter-api-v2": "^1.12.3",
    "undici": "^5.3.0",
    "utf-8-validate": "^5.0.9",
    "zlib-sync": "^0.1.7"
  },
  "imports": {
    "#lib/*": [
      "./dist/lib/*.js"
    ],
    "#structures/*": [
      "./dist/src/structures/*.js"
    ],
    "#models/*": [
      "./dist/src/models/*.js"
    ],
    "#listeners/*": [
      "./dist/src/listeners/*.js"
    ],
    "#constants/*": [
      "./dist/src/constants/*.js"
    ],
    "#interaction-handlers/*": [
      "./dist/src/interaction-handlers/*.js"
    ],
    "#routes/*": [
      "./dist/src/routes/*.js"
    ],
    "#config": [
      "./dist/config.js"
    ],
    "#itemList": [
      "./dist/lib/utils/itemList.js"
    ]
  },
  "scripts": {
    "start": "npm run build && pm2 start dist/ecosystem.config.cjs",
    "start:prod": "npm run build && pm2 start dist/ecosystem.config.cjs --env production",
    "build": "npm i && npm run cleanup && tsc --project tsconfig.json",
    "cleanup": "rm -rf dist",
    "setup": "npm run lint:fix && npm run prettier",
    "prettier": "prettier --write config.ts src/**/*.ts lib/**/*.ts",
    "lint": "eslint --ext .ts lib src",
    "lint:fix": "eslint --ext .ts --fix lib src",
    "prepare": "husky install"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.0.4",
    "@types/node-schedule": "^2.1.0",
    "@types/source-map-support": "^0.5.4",
    "@types/twitter": "^1.7.0",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "pm2": "^5.2.0",
    "prettier": "^2.7.1",
    "prisma": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
