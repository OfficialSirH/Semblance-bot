{
  "name": "semblance",
  "version": "1.0.0",
  "description": "The AI that runs the Reality Engine",
  "main": "dist/index.js",
  "author": "SirH",
  "type": "module",
  "dependencies": {
    "@top-gg/sdk": "^3.1.2",
    "bufferutil": "^4.0.5",
    "discord.js": "github:OfficialSirH/discord.js",
    "dotenv": "^10.0.0",
    "erlpack": "github:discord/erlpack",
    "fastify": "^3.22.1",
    "mongoose": "^6.0.7",
    "node-fetch": "^3.0.0",
    "pm2": "^5.1.2",
    "source-map-support": "^0.5.20",
    "twitter": "^1.7.1",
    "twitter.js": "^0.7.0-dev",
    "utf-8-validate": "^5.0.5",
    "zlib-sync": "^0.1.7"
  },
  "imports": {
    "#src/*": [
      "./dist/src/*.js"
    ],
    "#lib/*": [
      "./dist/lib/*.js"
    ],
    "#structures/*": [
      "./dist/src/structures/*.js"
    ],
    "#models/*": [
      "./dist/src/models/*.js"
    ],
    "#events/*": [
      "./dist/src/events/*.js"
    ],
    "#constants/*": [
      "./dist/src/constants/*.js"
    ],
    "#routing/*": [
      "./dist/src/routing/*.js"
    ],
    "#config": [
      "./dist/config.js"
    ],
    "#itemList": [
      "./dist/lib/utils/itemList.js"
    ]
  },
  "scripts": {
    "start": "npm run build && pm2 start --name \"Semblance\" dist/ecosystem.config.js",
    "reload": "npm run build && pm2 reload dist/ecosystem.config.js",
    "build": "npm i && cd node_modules/discord.js && gen-esm-wrapper ./src/index.js ./src/index.mjs",
    "prettier": "prettier --write src/**/*.ts lib/**/*.ts"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@types/module-alias": "^2.0.1",
    "@types/node-fetch": "^2.5.10",
    "@types/source-map-support": "^0.5.4",
    "@types/twitter": "^1.7.0",
    "@types/ws": "^7.4.1",
    "gen-esm-wrapper": "^1.1.3",
    "prettier": "^2.4.1",
    "typescript": "^4.5.0-dev.20210925"
  }
}
