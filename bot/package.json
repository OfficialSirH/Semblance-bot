{
  "name": "semblance",
  "version": "1.0.0",
  "description": "The AI that runs the Reality Engine",
  "main": "dist/src/index.js",
  "author": "SirH",
  "type": "module",
  "dependencies": {
    "@discordjs/collection": "^1.3.0",
    "@discordjs/core": "^0.3.0",
    "@discordjs/rest": "^1.5.0",
    "@discordjs/ws": "^0.6.0",
    "@prisma/client": "^4.7.0",
    "@sapphire/framework": "^3.2.0-pr-512.244de39.0",
    "@top-gg/sdk": "^3.1.3",
    "bufferutil": "^4.0.7",
    "dotenv": "^16.0.3",
    "fastify": "^4.10.2",
    "node-schedule": "^2.1.0",
    "source-map-support": "^0.5.21",
    "tweetnacl": "^1.0.3",
    "undici": "^5.13.0",
    "utf-8-validate": "^5.0.10",
    "zlib-sync": "^0.1.7"
  },
  "imports": {
    "#lib/*": [
      "./dist/lib/*.js"
    ],
    "#structures/*": [
      "./dist/src/structures/*.js"
    ],
    "#models/*": [
      "./dist/src/models/*.js"
    ],
    "#listeners/*": [
      "./dist/src/listeners/*.js"
    ],
    "#constants/*": [
      "./dist/src/constants/*.js"
    ],
    "#interaction-handlers/*": [
      "./dist/src/interaction-handlers/*.js"
    ],
    "#routes/*": [
      "./dist/src/routes/*.js"
    ],
    "#itemList": [
      "./dist/lib/utils/itemList.js"
    ]
  },
  "scripts": {
    "build-image": "docker build -t sirh/c2s:semblance .",
    "push-image": "docker push sirh/c2s:semblance",
    "start:docker": "node dist/src/index.js",
    "build:docker": "npx prisma generate && npx tsc --project tsconfig.json",
    "cleanup": "rm -rf dist",
    "setup": "npm run lint:fix && npm run prettier",
    "prettier": "prettier --write src/**/*.ts lib/**/*.ts",
    "lint": "eslint --ext .ts lib src",
    "lint:fix": "eslint --ext .ts --fix lib src",
    "prepare": "husky install"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.10",
    "@types/node-schedule": "^2.1.0",
    "@types/source-map-support": "^0.5.6",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.0",
    "prisma": "^4.7.0",
    "typescript": "^4.9.3"
  },
  "packageManager": "yarn@3.2.2"
}
